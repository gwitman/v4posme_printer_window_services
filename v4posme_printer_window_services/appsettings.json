{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "quartz": {
    "scheduler": {
      "instanceName": "QuartzDotnetAutofacExample"
    },
    "threadPool": {
      "type": "Quartz.Simpl.SimpleThreadPool, Quartz",
      "threadCount": "3",
      "threadPriority": "Normal"
    }
  },

  "GlobalSettings": {

    /*configuracion de servicio de impresion*/
    "FolderPath": "E:\\Users\\jmigu\\Downloads",
    "PrinterName": "POSDEMOPRINTER",
    "WidthPage": 283, /*centecimas de plugadas 283 = 2.83 pulgadas*/
    "PrefijoName": "posme_",
    "PrintIntervalSeconds": 2,
    "TipoPrinter": "POS",
    "Scale": 0,
    "Copies": 1,
    "PrintIntervalMinutes": 2,
    "MinutosMaximosParaImprimir": 1,

    /*configuracion de servicio ejecucion de .exe cada sierto tiempo*/
    "ProgramaHuellaIntervalSeconds": 2,
    "EnablePrinterJos": true,
    "EnableProgramaHuella": true,
    "NombreProgramaHuella": "WINWORD.EXE",
    "RutaProgramaHuella": "C:\\Program Files\\Microsoft Office\\root\\Office16\\",

    /*configuracion lista de servicios*/
    "Jobs": {
      "PrinterJob": {
        "Enabled": true,
        "Intervalo": 2
      },
      "ProgramaHuellaJob": {
        "Enabled": true,
        "Intervalo": 2
      }
    }


  }


}
